+            {
+                rc = buslogicR3ProcessMailboxNext(pThis);
+                AssertMsg(RT_SUCCESS(rc) || rc == VERR_NO_DATA, ("Processing mailbox failed rc=%Rrc\n", rc));
+            } while (RT_SUCCESS(rc));
+        }
+    } /* While running */
+
+    return VINF_SUCCESS;
